---
import PageHeading from "../../components/PageHeading.astro";
import VideosList from "../../components/vue/VideosList.vue";
import Layout from "../../layouts/Layout.astro";
---

<Layout>
  <section class="relative z-20 max-w-xl mx-auto my-12 px-7 lg:px-0">
    <PageHeading
      title="Videos"
      description="Acá están los últimos 5 videos publicados en mi canal de YouTube. Si te gusta lo que ves te animo a visitar mi canal o si tienes alguna sugerencia igualmente agradezco el feedback."
    />
    <VideosList client:load />
  </section>
</Layout>
